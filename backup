 email = request.form.get('email')
    password = request.form.get('password')

    try:
        user = firebase.auth().sign_in_with_email_and_password(email, password)
        # If the login is successful, redirect to the dashboard page and show SweetAlert popup
        return redirect(url_for('views.dash_view', success_message='Welcome to angels paradise'))
    except requests.exceptions.HTTPError:
        # Handle the authentication error, such as displaying an error message to the user
        return render_template('login.html', error_message="Bad credentials contact the administrator")


